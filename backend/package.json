{
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "description": "",
  "devDependencies": {
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3"
  },
  "main": "server.js",
  "name": "boc_storefront_backend",
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "./db/**",
      "*.sqlite",
      "*.sqlite-journal"
    ]
  },
  "private": true,
  "scripts": {
    "db:init": "npm run db:migrate && npm run db:seed",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:clean": "sequelize-cli db:seed:undo:all",
    "serve": "nodemon server.js"
  },
  "version": "0.0.1"
}
